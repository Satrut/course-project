 package login
+
+import (
+	"course-project/types"
+	"github.com/gin-gonic/gin"
+)
+
+func Login(c *gin.Context) {
+	request := types.LoginRequest{}
+	c.BindJSON(&request)
+}
+
+func Logout(c *gin.Context) {
+	c.JSON(200, gin.H{
+		"message": "Hello golang!",
+	})
+}
+
+func WhoAmI(c *gin.Context) {
+	c.JSON(200, gin.H{
